skip WS '\s+'
token COMMENT '<^#\w+>.+$'
token SKIP 'skip.+$'
token TOKEN 'token.+$'
token DIVIDER '%'
token NONTERM '<\w+>.+$'
%
<q2> ::= <line> 
<line>:Commentr ::= COMMENT
<line>:Dividerr ::= DIVIDER
<line>:Toker ::= <tokerrr>
<line>:Nontermer ::= NONTERM

<tokerrr>:Tokerrrr ::= TOKEN
<tokerrr>:Skipperr ::= SKIP
